{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "configVersion": {
      "type": "integer"
    },
    "settings": {
      "type": "object",
      "properties": {
        "allowed_save_file_formats": {
          "type": "array",
          "items": [
            {
              "type": "string"
            },
            {
              "type": "string"
            }
          ]
        },
        "canvas_size": {
          "type": "object",
          "properties": {
            "height": {
              "type": "integer",
              "minimum": 0
            },
            "weight": {
              "type": "integer",
              "minimum": 0
            }
          },
          "required": [
            "height",
            "weight"
          ]
        }
      }
    },
    "permisssions": {
      "type": "object"
    }
  },
  "required": [
    "configVersion",
    "settings",
    "permisssions"
  ]
}